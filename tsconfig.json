{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2020",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "outDir": "./dist/bot/"
  },
  "include": [
    "bot/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
