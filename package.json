{
  "name": "expend-bot",
  "version": "1.0.0",
  "description": "",
  "main": "bot/app.ts",
  "scripts": {
    "start": "npm run bot:run",
    "bot:quickstart": "ts-node -r dotenv/config -P tsconfig.json bot/app.ts",
    "build": "npm run web:compile && npm run bot:compile",
    "run": "npm run build && npm run bot:run",
    "clean": "gulp clean",
    "bot:compile": "tsc --project tsconfig.json",
    "web:compile": "gulp pack && tsc --project tsconfig.web.json",
    "bot:run": "node -r dotenv/config ./dist/bot/app.js"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "pg": "^8.7.1",
    "swig-templates": "^2.0.3",
    "telegraf": "^4.6.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/node": "^16.11.20",
    "consolidate": "^0.16.0",
    "del": "^6.0.0",
    "express": "^4.17.2",
    "gulp": "^4.0.2",
    "moment": "^2.29.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
