<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Info</title>
    <link rel="stylesheet" type="text/css" href="/info/info.css">
</head>
<body>
    <div id="main">
        <div class="categories">

            <template repeat="{{category in outcomes}}">

                <div class="category">
                    <div class="name">{{category.name}}</div>
                    <div class="aliases">{{category.aliases}}</div>
                    <div class="articles">
                        <template repeat="{{article in category.children}}">
                            <div class="name">
                                {{article.name}}
                            </div>
                            <div class="aliases">
                                {{article.aliases}}
                            </div>
                        </template>
                    </div>
                </div>

            </template>

            <template repeat="{{category in incomes}}">

                <div class="category">
                    <div class="name">{{category.name}}</div>
                    <div class="aliases">{{category.aliases}}</div>
                    <div class="articles">
                        <template repeat="{{article in category.children}}">
                            <div class="name">
                                {{article.name}}
                            </div>
                            <div class="aliases">
                                {{article.aliases}}
                            </div>
                        </template>
                    </div>
                </div>

            </template>

        </div>
    </div>
    <!--script src="./info.js"></script-->
</body>
</html>
